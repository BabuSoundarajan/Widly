@model IEnumerable<Widly.Models.Movie>


<br/>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                Movies
            </th>
            <th>
                Genre
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Name</td>
                <td>@movie.GenreType.Name</td>
            </tr>
        }
       
    </tbody>
</table>
